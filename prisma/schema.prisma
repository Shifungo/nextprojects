// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Activity {
  id           Int      @id @default(autoincrement())
  type         String
  start_time   String
  end_time     String
  moneyChange String
  description  String
}
